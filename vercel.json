{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://points.buildonhybrid.com/api/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://points.buildonhybrid.com/api/:path*/"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}